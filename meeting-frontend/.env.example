# Backend API URL
# В режиме разработки оставьте пустым (используется proxy)
# В продакшене укажите URL вашего backend сервера
REACT_APP_API_URL=

# Яндекс.Карты API ключ
# Получите ключ по инструкции в ../YANDEX_MAPS_SETUP.md
REACT_APP_YANDEX_MAPS_KEY=your_yandex_maps_api_key_here

# Яндекс.Метрика — номер счётчика (оставьте пустым для отключения)
REACT_APP_YANDEX_METRIKA_ID=

# Контакты (отображаются в футере)
REACT_APP_CONTACT_EMAIL=contact@tygde.ru
REACT_APP_CONTACT_TELEGRAM=
